{
  "mcpServers": {
    "time": {
      "command": "uvx",
      "args": [
        "--from",
        "./src/time-mcp-server",
        "time-mcp-server"
      ]
    },
    "item": {
      "url": "http://localhost:8000/mcp"
    },
    "item-stdio": {
      "command": "uvx",
      "args": [
        "mcp-proxy",
        "http://localhost:8000/mcp"
      ]
    }
  }
}
